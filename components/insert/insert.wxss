
/* 
 * insert
 */

.insert{
  width: 100%;
  height: 100vh;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, .6);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.insert navigator{
  position: relative;
}
.insert navigator image{  
  display: block;
  width: 600rpx;
  height: 800rpx;
}
.close image{
  width: 60rpx;
  height: 110rpx;
}
.adText{
  width: 50rpx;
  height: 30rpx;
  line-height: 30rpx;
  color: #fff;
  background-color: rgba(0,0,0,0.4);
  font-size: 18rpx;
  text-align: center;
  position: absolute;
  bottom: 0;
  right: 0;
}